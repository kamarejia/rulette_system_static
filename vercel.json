{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/*",
      "use": "@vercel/static"
    },
    {
      "src": "css/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/script.js",
      "dest": "/js/script.js"
    },
    {
      "src": "/style.css",
      "dest": "/css/style.css"
    },
   
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
